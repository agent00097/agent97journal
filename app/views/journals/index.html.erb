<h1> All your Journals </h1>

<table>
    <thead>
        <tr>
            <th>Date</th>
            <th>Title</th>
            <th>Actions</th>
        </tr>
    </thead>

    <tbody>
    <% @journals.each do |journal| %>
        <tr>
            <td><%= journal.created_at %> </td>
            <td><%= journal.title %> </td>
            <td>Placeholder</td>
        </tr>
        <% end %>
    </tbody>
</table>